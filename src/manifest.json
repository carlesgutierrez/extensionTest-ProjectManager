{
  "manifest_version": 3,
  "name": "URL Tracker and Project Manager",
  "version": "1.0.0",
  "description": "Captures URLs and organizes them by projects",
  "permissions": [
    "tabs",
    "storage",
    "activeTab"
  ],
  "action": {
    "default_popup": "index.html"
  },
  "background": {
    "service_worker": "background.ts",
    "type": "module"
  }
}